<odoo>
    <record id="action_import_wizard" model="ir.actions.act_window">
        <field name="name">Import Excel Wizard</field>
        <field name="res_model">import.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <record id="view_import_wizard_form" model="ir.ui.view">
        <field name="name">import.wizard.form</field>
        <field name="model">import.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Excel">
                <group>
                    <field name="filename" invisible="1" />
                    <field name="file" filename="filename" />

                </group>
                <footer>
                    <button string="Import" type="object" name="import_file" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <menuitem id="menu_excel_data" name="Excel Data" sequence="1"/>
    <menuitem id="menu_import_excel" name="Import Excel" parent="menu_excel_data" action="action_import_wizard"/>
</odoo>
